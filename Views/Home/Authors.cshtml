@model IEnumerable<N7.Models.AppUser>
@{
    ViewBag.Title = SharedLocalizer["Site Authors"];
}

<h1>@SharedLocalizer["Site Authors"]</h1>

<table class="table">

    @foreach (var user in Model)
    {
        <tr>            
            <td>
                <a asp-action="FindCollections" asp-route-AuthorId="@user.Id">@user.UserName</a>
            </td>
        </tr>
    }
</table>


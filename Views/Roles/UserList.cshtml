@model IEnumerable<N7.Models.AppUser>
@{
    ViewBag.Title = "Users and Roles";
}
<h2>Users</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Roles...</a>
            </td>
        </tr>
    }
</table>
